##
# Autostart programs
##

# D-Bus environment variables
exec dbus-update-activation-environment DISPLAY I3SOCK SWAYSOCK WAYLAND_DISPLAY XDG_CURRENT_DESKTOP=sway

# Notification daemon
exec dunst

# Polkit agent / xsettings daemon / autostart desktop entries
exec lxsession -s wayland -e sway

# vim: syntax=swayconfig
